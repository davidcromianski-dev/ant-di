{
  "name": "@davidcromianski-dev/ant-di",
  "version": "1.0.1",
  "description": "Simple JavaScript Dependency Injection Container inspired by PHP Pimple with advanced features like auto-wiring and service providers",
  "keywords": [
    "dependency-injection",
    "di",
    "container",
    "typescript",
    "javascript",
    "auto-wiring",
    "service-provider",
    "singleton",
    "factory",
    "pimple"
  ],
  "author": {
    "name": "David Cromianski",
    "email": "davidcromianski.dev@gmail.com",
    "url": "https://github.com/davidcromianski-dev"
  },
  "license": "MIT",
  "homepage": "https://github.com/davidcromianski-dev/ant-di#readme",
  "bugs": {
    "url": "https://github.com/davidcromianski-dev/ant-di/issues"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "!dist/**/*.test.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/davidcromianski-dev/ant-di.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "vite build",
    "prepublishOnly": "pnpm build",
    "test": "poku",
    "examples": "npx ts-node examples/examples.ts",
    "prepublish": "pnpm build && pnpm test",
    "publish:npm": "npm publish",
    "publish:dry-run": "npm publish --dry-run"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "poku": "^3.0.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4"
  }
}
